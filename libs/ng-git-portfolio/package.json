{
  "name": "@tehw0lf/ng-git-portfolio",
  "version": "0.0.10",
  "license": "MIT",
  "homepage": "https://github.com/tehw0lf/ng-git-portfolio.git",
  "repository": {
    "type": "git",
    "url": "https://github.com/tehw0lf/ng-git-portfolio.git"
  },
  "scripts": {
    "build": "../../node_modules/.bin/tsc -p tsconfig.schematics.json",
    "copy:schemas": "cp --parents schematics/*/schema.json ../../dist/ng-git-portfolio/",
    "copy:files": "cp -r --parents -p schematics/*/files/** ../../dist/ng-git-portfolio/",
    "copy:collection": "cp schematics/collection.json ../../dist/ng-git-portfolio/schematics/collection.json",
    "postbuild": "npm run copy:schemas && npm run copy:files && npm run copy:collection"
  },
  "peerDependencies": {
    "@angular/animations": "^12.1.1",
    "@angular/cdk": "^12.1.1",
    "@angular/common": "^12.1.1",
    "@angular/core": "^12.1.1",
    "@angular/flex-layout": "^12.0.0-beta.34",
    "@angular/material": "^12.1.1",
    "github-language-colors": "^1.0.0",
    "@primer/octicons": "^14.2.2"
  },
  "dependencies": {
    "tslib": "^2.2.0"
  },
  "schematics": "./schematics/collection.json",
  "ng-add": {
    "save": "dependencies"
  }
}
